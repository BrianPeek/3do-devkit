dir}makeone 3d_lib_demos 3d_stereo {1}{rootdir}makeone 3d_lib_demos perftest_3d {1}{rootdir}makeone 3d_lib_demos showmodel {1}directory {rootdir}##directory :Jumpstart2:{rootdir}makeone JSAnimation: JSAnimation {1}{rootdir}makeone JSInteractiveMusic JSInteractiveMusic {1}{rootdir}makeone JSInteractiveMusic JSIntMusicThread {1}{rootdir}makeone JSInteractiveMusic JSPlayBgndMusic {1}{rootdir}makeone JSInteractiveSound JSInteractiveSound {1}{rootdir}makeone JSMoveCel JSMoveCel {1}{rootdir}makeone JSShowCel JSShowCel {1}{rootdir}makeone JSSlideShow JSBasicSlideShow {1}{rootdir}makeone JSSlideShow JSSlideShowVDL {1}directory {rootdir}##directory :Streaming_1.9a7BuildDS clean "{rootdir}Streaming_1.9a7:"#directory {rootdir}{rootdir}makeone 3DOBounce bounce {1} {rootdir}makeone 3DOBounceSnd bouncesound {1} {rootdir}makeone 3DOOrbit 3DOOrbit  {1}{rootdir}makeone aaPlayer aaPlayer  {1}{rootdir}makeone "'Access Manager Test'" AccessTest  {1}{rootdir}makeone AnimSample Example2 {1}{rootdir}makeone AssetManager TestAM {1} {rootdir}makeone ColorEcho ColorEcho {1} {rootdir}makeone FontLibrary FontLibExample {1} {rootdir}makeone JoyJoy EventJoy {1} {rootdir}makeone LREX LREX {1} {rootdir}makeone Menu Menu {1} {rootdir}makeone MemTest MemTest {1} {rootdir}makeone MessageSample MessageSample {1} {rootdir}makeone MessageSample MessageClient {1} {rootdir}makeone PerfTest PerfTest {1} {rootdir}makeone SlideShow SlideShow {1} {rootdir}makeone SlideShow Example2VDL {1} {rootdir}makeone SymAnim SymAnim {1} {rootdir}makeone TaskDemo TaskDemo {1} {rootdir}makeone UFO UFO {1} {rootdir}makeone UFO PlayBgndMusic {1} {rootdir}makeone UserFolioTemplate ft_folio {1} {rootdir}makeone UserFolioTemplate ft_test {1} {rootdir}makeone VersionChecker VersionChecker {1}echoecho "Build of " {rootdir}Examples completedecho "Executables and symbols have been copied into" {3DORemote}echo "You may need to copy data files from the " '"Apps & Data"' echo "folder on the release CD into" {3DORemote}echo echo "Run Menu to try the examples" echo "See menu.script for sample command lines"unset 3DOAutodup